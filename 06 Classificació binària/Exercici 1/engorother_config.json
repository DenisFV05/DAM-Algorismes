{
    "config_path": "engorother_config.json",
    "model_path": "engorother_model.pth",
    "vocab_path": "engorother_vocab.json",
    "csv_path": "./data/engorother.csv",
    "training": {
        "batch_size": 32,
        "epochs": 25,
        "learning_rate": 0.001,
        "validation_split": 0.2
    },
    "model_params": {
        "hidden_size": 256,
        "dropout_rate": 0.5,
        "num_output": 1
    },
    "early_stopping": {
        "patience": 5,
        "min_delta": 0
    },
    "reduce_lr_on_plateau": {
        "mode": "min",
        "factor": 0.1,
        "patience": 3
    },
    "classes": [
        "eng",
        "other"
    ]
}